<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.auth.audit.LoginLogMapper">

    <insert id="insert" parameterType="org.example.auth.audit.LoginLog"
            useGeneratedKeys="true" keyProperty="id">
        INSERT INTO login_logs (
            user_id,
            identifier,
            channel,
            ip,
            user_agent,
            status,
            created_at
        ) VALUES (
            #{userId},
            #{identifier},
            #{channel},
            #{ip},
            #{userAgent},
            #{status},
            #{createdAt}
        )
    </insert>

</mapper>
